{
  "name": "react-ebi-species",
  "version": "1.2.4",
  "description": "React component based on the EBI species icons",
  "main": "lib/index.js",
  "scripts": {
    "dist": "NODE_ENV=production webpack --optimize-minimize",
    "prepublishOnly": "rm -rf lib && babel src -d lib --copy-files",
    "test": "jest --coverage",
    "posttest": "./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info  || echo 'Coveralls upload failed, LOL ignoring'"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "author": "Wojciech Bazant <wbazant@ebi.ac.uk>",
  "contributors": [
    "Alfonso Mu√±oz-Pomer Fuentes <amunoz@ebi.ac.uk>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gxa/react-ebi-species"
  },
  "keywords": [
    "react",
    "icon",
    "ebi",
    "species",
    "ensembl",
    "bioinformatics",
    "biology",
    "rabbit",
    "chicken"
  ],
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "jest": "^20.0.4",
    "react-test-renderer": "^15.6.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  }
}
