{
  "name": "react-ebi-species",
  "version": "2.2.0",
  "description": "React component based on the EBI species icons",
  "main": "lib/index.js",
  "scripts": {
    "prepack": "rm -rf lib && babel src -d lib --copy-files",
    "test": "jest",
    "posttest": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "postversion": "npm publish",
    "postpublish": "git push && git push --tags"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "author": "Alfonso Mu√±oz-Pomer Fuentes <amunoz@ebi.ac.uk>",
  "contributors": [
    "Wojciech Bazant <wbazant@ebi.ac.uk>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ebi-gene-expression-group/react-ebi-species"
  },
  "keywords": [
    "react",
    "icon",
    "ebi",
    "species",
    "ensembl",
    "bioinformatics",
    "biology",
    "rabbit",
    "chicken"
  ],
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.2",
    "coveralls": "^3.0.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "jest": "^24.8.0",
    "jest-environment-enzyme": "^7.0.2",
    "jest-enzyme": "^7.0.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  }
}
