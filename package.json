{
  "name": "react-ebi-species",
  "version": "2.0.0",
  "description": "React component based on the EBI species icons",
  "main": "lib/index.js",
  "scripts": {
    "prepack": "rm -rf lib && babel src -d lib --copy-files",
    "testOnly": "jest --verbose --coverage",
    "test": "npm run testOnly",
    "posttest": "./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info  || echo 'Coveralls upload failed'",
    "postversion": "npm publish",
    "postpublish": "git push && git push --tags"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "author": "Alfonso Mu√±oz-Pomer Fuentes <amunoz@ebi.ac.uk>",
  "contributors": [
    "Wojciech Bazant <wbazant@ebi.ac.uk>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ebi-gene-expression-group/react-ebi-species"
  },
  "keywords": [
    "react",
    "icon",
    "ebi",
    "species",
    "ensembl",
    "bioinformatics",
    "biology",
    "rabbit",
    "chicken"
  ],
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "styled-components": "^4.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "coveralls": "^3.0.2",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "jest": "^23.6.0",
    "react-test-renderer": "^16.5.2",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  }
}
