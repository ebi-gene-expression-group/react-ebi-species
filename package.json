{
  "name": "react-ebi-species",
  "version": "1.1.0",
  "description": "React component based on the EBI species icons",
  "main": "lib/index.js",
  "scripts": {
    "dist": "NODE_ENV=production webpack --optimize-minimize",
    "build": "babel src -d lib --copy-files",
    "prepublish": "rm -rf lib && npm run build && npm run dist",
    "test": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "author": "Wojciech Bazant <wbazant@ebi.ac.uk> Alfonso Mu√±oz-Pomer Fuentes <amunoz@ebi.ac.uk>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wbazant/react-ebi-species"
  },
  "keywords": [
    "react",
    "icon",
    "ebi",
    "species",
    "ensembl",
    "bioinformatics",
    "biology",
    "rabbit",
    "chicken"
  ],
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.4",
    "jest": "^20.0.4",
    "react-hot-loader": "^3.0.0-beta.7",
    "style-loader": "^0.18.2",
    "webpack": "^2.6.0",
    "webpack-dev-server": "^2.5.0"
  }
